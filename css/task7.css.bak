*{	
			margin:0;
			padding:0;
			font-family:"微软雅黑";
		}
		body{
			min-width: 1280px;
		}
		a{
			text-decoration:none;
		}
		.header{/*不能设置宽度为100%*/
			height:57px;
		}
		.part1{/*默认和浏览器窗口大小一样*/
			height:855px;
			clear:both;
			position:relative;
		}
		.part2{
			height:250px;
		}
		.part3{
			height:360px;
			background:#ededed;
		}
		.part4{
			height:341px;
			position:relative;/*加遮罩层用定位*/
			overflow:hidden;/*解决子元素div.content设了margin-top之后父元素所受的影响*/
		}
		.part5{
			height:605px;
			background:#ededed;
			overflow:hidden;/*解决子元素ul设了margin-top之后父元素所受的影响*/
		}
		.part6{
			height:567px;
			position:relative;
		}
		.part7{
			height:520px;
		}
		.part8{
			height:750px;
			overflow:hidden;
		}
		.footer{
			height:415px;
			background:#32363e;
		}
		.header .pic{
			width:120px;
			height:40px;
			margin-left:55px;
			padding:10px;
			line-height:40px;
			float:left;
		}
		/*导航栏部分*/
		.header img{/*使文字和图片垂直对齐*/
			vertical-align:middle;
		}
		.header .pic span{
			font-size:21px;
			font-family:"黑体";
			margin-left:5px;
		}
		.header .pic span{
			font-size:21px;
			font-family:"黑体";
			margin-left:5px;
		}
		.header ul{
			list-style:none;
			float:right;
			margin-right:40px;
		}
		.header ul li{
			float:left;
			margin-right:20px;
			text-align:center;
			line-height:55px;
		}
		.header ul li:hover{
			border-bottom:3px solid #e74f4d;
		}
		.header ul li a{
			display:block;
			width:65px;
			height:57px;
			color:#787b83;
			font-size:16px;
			font-family:"微软雅黑";
		}
		.header ul li a:hover{
			color:#e74f4d;	
		}
		.part1 img{
			width:100%;
			height:100%;
		}
		.part1 .text{
			position:absolute;
			left:125px;
			top:400px;
			z-index:1;
		}
		.part1 .text p.t1{
			font-size:60px;
			font-family:"微软雅黑";
			color:#fff;
		}
		.part1 .text p.t2{
			margin-top:20px;
			font-size:21px;
			font-family:"微软雅黑";
			color:#fff;
		}
		.part1 .text p.t3{
			width:330px;
			height:65px;
			margin-top:30px;
			text-align:center;
			line-height:65px;
			font-size:30px;
			font-family:"微软雅黑";
			color:#fff;
			background:#e74f4d;
			cursor:pointer;
		}
		.part1 .mask{
			height:100%;/*缩放的时候才会覆盖img*/
			width:100%;
			background:#ccc;
			position:absolute;
			top:0;
			left:0;
			opacity:0.3;
		}
		.part2 ul{
			list-style:none;
			text-align:center;
			overflow:hidden;/*撑大父元素的高度*/
		}
		.part2 ul li{
			float:left;
			width:320px;
			margin-top:80px;
			font-size:14px;
			color:#767777;
			font-family:"黑体";
		}
		.part2 ul li p{
			width:155px;/*设置内容宽度*/
			margin:30px auto;
			padding:0 82.5px;/*增加内边距,因此可以设置border-right*/
			border-right:1px solid #767777;
		}
		.part3{
			text-align:center;
			font-size:50px;
			font-family:"黑体";
			color:#a1a1a1;
		}
		.part3 img{
			margin-top:85px;
		}
		.part4 img{
			position:absolute;
			z-index:-3;
			width:100%;
		}
		.part4 .mask{
			height:100%;
			width:100%;
			background:#f00;
			position:absolute;
			top:0;
			left:0;
			opacity:0.1;
			z-index:-2;
		}
		.part4 .content{
			width:1000px;
			margin:25px auto;
			text-align:center;
		}
		.part4 .content .tx1{
			color:#fff;
			font-size:23px;
			cursor:pointer;
		}
		.part4 .content .underline{
			width:25px;
			height:10px;
			margin:0 auto;
			border-bottom:2px solid #fff;
			transition:all 0.5s;/*过渡属性*/
		}
		.tx1:hover~.underline{/*必须放在初始样式后面*/
			width:280px;
		}
		.part4 .content .tx2{
			color:#fff;
			margin:20px 0;
			font-size:12px;
		}
		.part4 .content select{
			width:235px;
			height:40px;
			color:#ccc;
			font-size:12px;
		}
		.part4 .content input{
			width:235px;
			height:40px;
			background:#f75;
			color:#fff;
			font-size:14px;
			font-family:"微软雅黑";
			border:0;
		}
		.part5 ul{
			list-style:none;
			width:985px;
			height:390px;
			margin:107px auto;
			text-align:center;
			font-size:16px;
			font-family:"微软雅黑";
		}
		.part5 ul li{
			width:220px;
			float:left;
			margin:0 10px;
		}
		.part5 ul li img{/*图片是可以设置padding的,长见识了*/
			border:1px solid #e1e1e1;
			padding:10px 5px;
			background:#fff;
		}
		.part5 ul li .tx2{
			font-size:12px;
			font-family:"微软雅黑";
			color:#9f9f9f;
			margin-top:5px;
		}	
		.part6 .content-left{
			width:66.6%;
			height:567px;
			float:left;
			position:relative;
			background:url("../images_t7/man.png") no-repeat;
			background-size:cover;/*把背景图像扩展至足够大，以使背景图像完全覆盖背景区域。*/
			/*要在设置背景后面才有效*/
		}
		.part6 .content-left .words{
			color:#fff;
			font-family:"微软雅黑";
			position:absolute;
			left:240px;
			top:120px;
			text-align:center;
		}
		.part6 .content-left .words .tx1{
			font-size:50px;
		}
		.part6 .content-left .words .tx2{
			font-size:158px;
			line-height:160px;
		}
		.part6 .content-left .words .tx3{
			font-size:22px;
		}
		.part6 .content-right{
			width:33.3%;
			height:567px;
			float:right;
			overflow:hidden;
		}
		.part6 .content-right .words{
			width:300px;
			height:300px;
			float:right;
			text-align:center;
			position:relative;
			right:50px;
			top:120px;
		}
		.part6 .content-right .words h1{
			font-size:20px;
		}
		.part6 .content-right .words h1 span{
			color:#e74f4d
		}
		.part6 .content-right .words p{
			margin:30px 0;
			font-size:14px;
			color:#aaa;
			line-height:25px;
		}
		.part6 .content-right .words input{
			width:105px;
			height:30px;
			background:#fff;
			color:#e74f4d;
			border:1px solid #e74f4d;
			font-family:"微软雅黑";
		}
		.part6 .content-right .words input:hover{
			background:#e74f4d;
			color:#fff;
		}
		.part6 .content-right .words .circle{
			width:10px;
			height:10px;
			border-radius:50%;
			border:1px solid #aaaaa9;
			display:inline-block;
			margin-top:20px;
		}
		.part6 .triangle{
			position:absolute;
			top:250px;
			right:-1px;
		}
		.part7 ul{
			list-style:none;
			overflow:hidden;
			height:520px;
		}
		.part7 ul li{
			float:left;
			text-align:center;
			height:520px;
		}
		.part7 ul li.first{
			width:33.3%;
			color:#fff;
			background:#e74f4d;
		}
		.part7 ul li.first p{
			cursor:pointer;
			margin:10px 0;
		}
		.part7 ul li.first .underline{
			width:25px;
			height:10px;
			margin:10px auto;
			border-bottom:2px solid #fff;
			transition:all 0.5s;/*过渡属性*/
		}
		.part7 ul li.first p:hover~.underline{/*必须放在初始样式后面*/
			width:150px;
		}
		.part7 input{
			width:170px;
			height:45px;
			color:#fff;
			background:#e74f4d;
			font-size:21px;
			font-family:"微软雅黑";
			border:1px solid #fff;;
			cursor:pointer;
			margin-top:10px;
		}
		.part7 ul li.first input:hover{
			color:#e74f4d;
			background:#fff;
		}
		.part7 ul li.second{
			width:33.3%;;
			color:#e74f4d;
			background:#fff;
		}
		.part7 ul li.second p{
			cursor:pointer;
			margin:10px 0;
			color:#323333;
		}
		.part7 ul li.second .underline{
			width:25px;
			height:10px;
			margin:10px auto;
			border-bottom:2px solid #323333;
			transition:all 0.5s;/*过渡属性*/
		}
		.part7 ul li.second p:hover~.underline{/*必须放在初始样式后面*/
			width:150px;
		}
		.part7 ul li.second input{
			color:#e74f4d;
			background:#fff;
			border:1px solid #e74f4d;
		}
		.part7 ul li.second input:hover{
			color:#fff;
			background:#e74f4d;
		}
		.part7 ul li.third{
			width:33.3%;
			background:url("../images_t7/woman.jpg") no-repeat;
			background-size:cover;
			position:relative;
		}
		.part7 ul li.third .triangle{
			position:absolute;
			top:250px;
			left:-2px;
		}
		.part7 .content{
			margin-top:180px;
		}
		.part8 .title{
			width:33.3%;
			margin:120px auto;
			text-align:center;
		} 
		.part8 .title h3{
			font-family:"黑体";
			color:#323333;
			font-size:21px;
		} 
		.part8 .title p{
			color:#aaa;
			font-size:12px;
			margin-top:10px;
		} 
		.part8 .title .underline{
			width:25px;
			height:10px;
			margin:0px auto;
			border-bottom:2px solid #e74f4d;
			transition:all 0.5s;/*过渡属性*/
		}
		.part8 .title h3:hover~.underline{/*必须放在初始样式后面*/
			width:200px;
		}
		.part8 .content{
			padding-top:5px;/*使用外边距无效,跟上一个模块的外边距重合*/
			overflow:hidden;
		}
		.part8 .content-left{
			width:33.3%;
			float:left;
		}
		.part8 .content-left .words{
			width:65%;
			float:right;
		}
		.part8 .content-left .words h3{
			color:#7c7c7c;
			font-size:14px;
			font-family:"黑体";
			margin:20px 0;
		}
		.part8 .content-left .words p{
			color:#b5b3b3;
			font-size:12px;
			line-height:25px;
			font-family: "微软雅黑";
		}
		.part8 .content-left .words a{
			text-decoration:none;
			color:#aaa;
			font-size:12px;
		    font-family: "微软雅黑";
			float:right;
		}
		.part8 .content-left .words a:hover{
			color:#868686;
		}
		.part8 .content-right{
			width:60%;
			float:right;
		}
		.part8 .content-right form{
			width:70%;
			float:left;
		}
		.part8 .content-right form .txt{
			width:45%;
			height:40px;
			background:#ededed;
			border:0;
			margin:10px 10px 15px 0;
			padding-left:10px;
		}
		.part8 .content-right form textarea{
			resize:none;/*取消文本域右下角图标*/
			width:90%;
			background:#ededed;
			border:0;
			margin:10px 10px 10px 0;
			padding:10px 25px 10px 10px;
		}
		.part8 .content-right form .submit{
			width:96%;
			height:40px;
			background:#ededed;
			border:0;
			margin-top:10px;
		}
		.footer{
			color:#fff;
			font-family:"微软雅黑";
			text-align:center;
			overflow:hidden;
		}
		.footer .content{
			margin-top:80px;
		}
		.footer .content h3{
			font-size:21px;
			font-family:"黑体";
			margin:10px;
		}
		.footer .content p{
			font-size:12px;
			line-height:20px;
		}
		.footer .content form{
			width:50%;
			margin:10px auto;
		}
		.footer .content form{
			width:50%;
			margin:10px auto;
		}
		.footer .content form{
			width:50%;
			margin:20px auto;
			overflow:hidden;
		}
		.footer .content form .txt{
			width:60%;
			border:0;
			height:40px;
			padding:5px;
			box-sizing:border-box;
		}
		.footer .content form .submit{
			width:80px;
			height:40px;
			background:#6f7178;
			font-size:15px;
			color:#fff;
			font-family:"微软雅黑";
			border:0;
			margin-left:-5px;
			margin-top:1px;/*提交按钮和文本框上下对齐*/
		}
		.footer .content .pic img{
			margin:10px;
		}
		.footer .bottom{
			height:57px;
			background:#303039;
			margin-top:65px;
			color:#fff;
			font-size:12px;
			padding-top:20px;
		}
		.footer .bottom .txt{
			width:90%;
			margin:0 auto;
		}
		.footer .bottom .txt span{
			float:left;
		}
		.footer .bottom .txt a{
			float:right;
			color:#fff;
		}