*{
	padding:0;
	margin:0;
	box-sizing:border-box;
	outline:none;/*去掉表单元素获取焦点时候的边框*/
}
ul{
	list-style:none;
}
a{
	text-decoration:none;
}
body{
	min-width:1280px;/*设置最小宽度,当浏览器窗口小于设计稿宽度时,出现横向滚动条,页面内容宽度保持不变*/
}
textarea{
	resize:none;
}
select{
	/*清除select的默认三角形样式*/
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	/*清除select的默认三角形样式*/
	padding-left:10px;
	/*在选择框的最右侧中间显示小箭头图片*/
	background:url("../images_t9/tobottom.png") no-repeat 90% center/10px ;/*第一个值是水平位置，第二个值是垂直位置。*/	
}
input[type="date"]::-webkit-inner-spin-button{
	display:none;
}
input[type="date"]{
	padding-left:10px;
	box-sizing:border-box;
}
.header{
	height:57px;
	background:#2189bf;
}
.content{
	margin:0 auto;
	width:1280px;
	overflow:hidden;
}
#container .header img.logo{
	margin-left:50px;
	float:left;
	margin-top:10px;
}
#container .header .content{
	float:right;
	width:375px;
	margin-top:15px;
