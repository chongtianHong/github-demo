		.module5-left .project-item{
			position:relative;
			padding:40px 60px;
		}
		.project-item ul li{/*由于input和.tab定位都设置为absolute,脱离文档流,li的高度由span确定*/
			display:inline-block;
			position:relative;
			width:70px;
			line-height:30px;/*span元素高度*/
			text-align:center;
		}
		.project-item ul li input{
			position:absolute;
			width:100%;/*宽度继承li宽度*/
			height:100%;/*高度继承li高度*/
			opacity:0;/*单选框透明度设置为0*/
			z-index:99;/*单选框设置在最上层*/
			cursor:pointer;
			left:0;
		}
		.project-item ul li span{
			display:block;
			position:relative;
			width:100%;
			height:100%;
			background:#ededf0;
			border-top-left-radius:5px;/*向元素的左上角添加圆角边框：*/
			border-top-right-radius:5px;/*向元素的右上角添加圆角边框：*/
			z-index:1;
			font-size:12px;
			color:#535353;
			font-family:"微软雅黑";
		}
		.project-item ul li input:checked + span{/*+为相邻同胞选择器:*/
			background:#fff;
			border:1px solid #ddd;
			border-bottom:1px solid #fff;
			border-bottom-left-radius:0;
			border-bottom-right-radius:0;
		}
		.project-item ul li .tab{
			display:none;
			position:absolute;
			width:409px;
			height:175px;
			padding:10px 30px;
			top:30px;
			border:1px solid #ddd;
			z-index:0;
			border-radius:0 5px 5px 5px;
		}
		.project-item ul li .tab table{
			border:1px solid #ddd;
			border-collapse:collapse;
			width:350px;
			font-size:12px;
			color:#535353;
			font-family:"黑体";
			cursor:pointer;
		}
		.project-item ul li .tab table tr td:hover{
			background:#f4f4f4;
		}
		.project-item ul li .item-2{
			left:-75px;	
			border-radius:5px;
		}
		.project-item ul li .item-3{
			left:-150px;
			border-radius:5px;
		}
		.project-item ul li .item-4{
			left:-225px;
			border-radius:5px;
		}
		.project-item ul li .item-5{
			left:-300px;	
			border-radius:5px;
		}
		.project-item ul li input:checked + span + .tab{
			display:block;
		}
